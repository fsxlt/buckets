{"contradiction": [{"uid": "arabic-1166-val", "text_a": "\u062a\u0628\u062f\u0648 \u0628\u0644\u062f\u0629 \u0623\u0644\u0627\u064a\u0648\u0631 \u060c \u0648\u0647\u064a \u0645\u062c\u0645\u0648\u0639\u0629 \u0645\u0646 \u0627\u0644\u0628\u064a\u0648\u062a \u0627\u0644\u0628\u064a\u0636\u0627\u0621 \u0645\u062a\u062c\u0645\u0639\u0629 \u0639\u0644\u0649 \u062a\u0644\u0629 \u0645\u0646\u062e\u0641\u0636\u0629 \u060c \u0639\u0644\u0649 \u0645\u0633\u0627\u0641\u0629 \u0645\u062b\u0644 \u0642\u0631\u064a\u0629 \u0639\u0631\u0628\u064a\u0629 \u0623\u0648 \u0623\u0646\u062f\u0644\u0633\u064a\u0629 .", "label": "contradiction", "portion_identifier": -1, "text_b": "\u0623\u0644\u0627\u064a\u0648\u0631 \u0647\u064a \u0643\u0644 \u0627\u0644\u0628\u064a\u0648\u062a \u0627\u0644\u0633\u0648\u062f\u0627\u0621 \u0627\u0644\u0635\u063a\u064a\u0631\u0629 ."}], "entailment": [{"uid": "arabic-1097-val", "text_a": "\u0639\u0646\u062f\u0645\u0627 \u062a\u0645\u0631 \u0645\u0646 \u062e\u0644\u0627\u0644\u0647\u0645 \u0642\u062f \u062a\u0631\u0649 \u0645\u062f\u062e\u0646\u0629 \u0642\u062f\u064a\u0645\u0629 \u062a\u062a\u0643\u0633\u0631 \u0641\u0648\u0642 \u062e\u0637 \u0627\u0644\u0623\u0634\u062c\u0627\u0631 \u060c \u0648\u0647\u064a \u0639\u0644\u0627\u0645\u0629 \u0645\u0624\u0643\u062f\u0629 \u0639\u0644\u0649 \u0623\u0646 \u0627\u0644\u0642\u0631\u064a\u0629 \u0643\u0627\u0646\u062a \u0641\u064a \u064a\u0648\u0645 \u0645\u0646 \u0627\u0644\u0623\u064a\u0627\u0645 \u062c\u0632\u0621\u0627\u064b \u0645\u0646 \u0645\u0632\u0631\u0639\u0629 .", "label": "entailment", "portion_identifier": -1, "text_b": "\u062a\u062f\u0644 \u0627\u0644\u0645\u062f\u0627\u062e\u0646 \u0639\u0644\u0649 \u0623\u0646 \u0627\u0644\u0642\u0631\u064a\u0629 \u0643\u0627\u0646\u062a \u062c\u0632\u0621 \u0645\u0646 \u0645\u0632\u0631\u0639\u0629 ."}], "neutral": [{"uid": "arabic-1277-val", "text_a": "\u0627\u0646\u0647 \u0648\u0627\u062c\u0628 \u0645\u062f\u064a\u0631 \u0642\u0633\u0645 \u0627\u0644\u0645\u06