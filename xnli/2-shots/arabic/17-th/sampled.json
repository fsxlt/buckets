{"contradiction": [{"uid": "arabic-1794-val", "text_a": "\u062a\u0643\u0644\u0641\u0629 \u0625\u0646\u0634\u0627\u0621 \u0647\u0630\u0647 \u0627\u0644\u0645\u0646\u0627\u0632\u0644 \u062a\u062a\u062c\u0627\u0648\u0632 \u0628\u0643\u062b\u064a\u0631 \u0645\u0627 \u064a\u062f\u0641\u0639 \u0627\u0644\u0645\u0634\u062a\u0631\u064a\u0646 \u0644\u0646\u0627 \u060c \u0644\u0630\u0644\u0643 \u0646\u062d\u0646 \u0646\u0639\u062a\u0645\u062f \u0639\u0644\u0649 \u0627\u0644\u0645\u0646\u062d \u0648\u0627\u0644\u062a\u0628\u0631\u0639\u0627\u062a \u0627\u0644\u0641\u0631\u062f\u064a\u0629 \u0644\u0644\u062d\u0641\u0627\u0638 \u0639\u0644\u064a\u0647\u0627 \u0628\u0623\u0633\u0639\u0627\u0631 \u0645\u0639\u0642\u0648\u0644\u0629 .", "label": "contradiction", "portion_identifier": -1, "text_b": "\u0635\u0646\u0639 \u0627\u0644\u0645\u0646\u0627\u0632\u0644 \u0643\u0627\u0646 \u0645\u062c\u0627\u0646\u064a\u0627 \u0628\u0627\u0644\u0643\u0627\u0645\u0644 ."}, {"uid": "arabic-627-val", "text_a": "\u0633\u062a\u0635\u0628\u062d \u0643\u0628\u064a\u0631\u0629 \u062c\u062f\u064b\u0627 \u060c \u0648\u0633\u062a\u0623\u062e\u0630\u0646\u0627 \u0645\u0646 \u0641\u064f\u0631\u0634\u0646\u0627 \u0648\u0644\u0646 \u062a\u0639\u0648\u062f \u0628\u0646\u0627 \u0625\u0644\u0649 \u0627\u0644\u0648\u0637\u0646 ( \u0646\u0648\u0641\u0645\u0628\u0631 1974 ) .", "label": "contradiction", "portion_identifier": -1, "text_b": "\u0627\u0646\u0647\u0627 \u062f\u0627\u0626\u0645\u0627 \u062a\u0631\u062c\u0639\u0647\u0645 \u0625\u0644\u0649 \u0627\u0644\u0648\u0637\u0646 ."}], "entailment": [{"uid": "arabic-2336-val", "text_a": "\u0645\u0646 \u0627\u0644\u0644\u0627\u0641\u062a \u0644\u0644\u0646\u0638\u0631 \u0623\u0646 \u0647\u0630\u0627 \u0627\u0644\u0634\u0630\u0648\u0630 \u0644\u0627 \u064a\u0632\u0627\u0644 \u0642\u0627\u0626\u0645\u0627 \u060c \u062d\u062a\u0649 \u0641\u064a \u0627\u0644\u0639\u062f\u064a\u062f \u0645\u0646 \u0627\u0644\u0645\u0635\u0627\u062f\u0631 \u0627\u0644\u062d\u062f\u064a\