{"contradiction": [{"uid": "arabic-47-val", "text_a": "\u062e\u0633\u0631\u0646\u0627 \u0637\u0627\u0626\u0631\u062a\u064a\u0646 \u0623\u0648 \u062b\u0644\u0627\u062b\u0629 \u0639\u0646\u062f\u0645\u0627 \u0643\u0646\u0627 \u0647\u0646\u0627\u0643 \u060c \u0648 \u060c \u0625\u064a\u0647 \u060c \u0648\u0643\u0627\u0646\u062a \u0645\u0631\u062d\u0644\u0629 \u0627\u062e\u062a\u0628\u0627\u0631 .", "label": "contradiction", "portion_identifier": -1, "text_b": "\u0644\u0645 \u0646\u0641\u0642\u062f \u0623\u0628\u062f\u0627 \u0623\u0649 \u0637\u0627\u0626\u0631\u0629 ."}, {"uid": "arabic-788-val", "text_a": "\u0628\u0643\u0649 \u0623\u0648\u062c\u0644 \u0642\u0627\u0626\u0644\u064b\u0627 : \u0647\u0645 \u0641\u064a \u0627\u0644\u0645\u062f\u0649 .", "label": "contradiction", "portion_identifier": -1, "text_b": "\u0639\u0644\u0649 \u0627\u0644\u0631\u063a\u0645 \u0645\u0646 \u0623\u0646\u0647 \u0643\u0627\u0646 \u064a\u0639\u0644\u0645 \u0623\u0646\u0647\u0627 \u0643\u0627\u0646\u062a \u0636\u0645\u0646 \u0627\u0644\u0646\u0637\u0627\u0642 \u060c \u0625\u0644\u0627 \u0623\u0646 \u063a\u0645\u0632\u0627\u062d\u062a\u0641\u0638 \u0628\u0647\u0627 \u0644\u0646\u0641\u0633\u0647 ."}], "entailment": [{"uid": "arabic-1430-val", "text_a": "\u0646\u062d\u062a\u0627\u062c \u0627\u0644\u0649 \u0647\u0630\u0647 \u0627\u0644\u0645\u0648\u0627\u0631\u062f \u0627\u0644\u0627\u0636\u0627\u0641\u064a\u0629 \u0644\u0646\u0648\u0627\u0635\u0644 \u062c\u0647\u0648\u062f\u0646\u0627 \u0641\u0649 \u062f\u0639\u0645 \u0648\u062a\u0639\u0632\u064a\u0632 \u0642\u0648\u0629 \u0645\u0643\u062a\u0628 \u0627\u0644\u0645\u0633\u0627\u0621\u0644\u0629 \u0627\u0644\u062d\u0643\u0648\u0645\u064a\u0629 \u0644\u062a\u0643\u0648\u0646 \u0645\u0646\u0638\u0645\u0629 \u0646\u0645\u0648\u0630\u062c\u064a\u0629 \u0644\u0628\u0642\u064a\u0629 \u0627\u0644\u062d\u0643\u0648\u0645\u0629 \u0627\u0644\u0641\u064a\u062f\u0631\u0627\u0644\u064a\u0629 . \u0648\u062c\u0645\u064a\u0639 \u0645\u0643\u0627\u062a\u0628 \u06